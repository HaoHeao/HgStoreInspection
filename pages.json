{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "layout/index/index",
		"style": {
			"disableScroll": true
		}
	}],
	"subPackages": [{
		"root": "pages-packages/store",
		"pages": [{
				"path": "index/index",
				"style": {
					"navigationBarTitleText": "卖场巡检",
					"disableScroll": true
				}
			},
			{
				"path": "detail/detail",
				"style": {
					"navigationBarTitleText": "巡检记录",
					"disableScroll": true
				}
			},
			{
				"path": "discuss/discuss",
				"style": {
					"navigationBarTitleText": "问题讨论",
					"disableScroll": true
				}
			},
			{
				"path": "submitQuestions/submitQuestions",
				"style": {
					"navigationBarTitleText": "提出巡检问题",
					"disableScroll": true
				}
			}
		]
	}, {
		"root": "pages-packages/plan",
		"pages": [{
				"path": "index/index",
				"style": {
					"navigationBarTitleText": "计划巡检",
					"disableScroll": true
				}
			}, {
				"path": "plan/plan",
				"style": {
					"navigationBarTitleText": "计划巡检内容",
					"disableScroll": true
				}
			}, {
				"path": "submitQuestions/submitQuestions",
				"style": {
					"navigationBarTitleText": "提出巡检问题",
					"disableScroll": true
				}
			},
			{
				"path": "question/question",
				"style": {
					"navigationBarTitleText": "巡检问题",
					"disableScroll": true
				}
			}
		]
	}, {
		"root": "pages-packages/meeting",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "会议预约",
				"disableScroll": true
			}
		}, {
			"path": "reserva/index",
			"style": {
				"navigationBarTitleText": "预约",
				"disableScroll": true
			}
		}, {
			"path": "detail/index",
			"style": {
				"navigationBarTitleText": "预约信息",
				"disableScroll": true
			}
		}]
	}, {
		"root": "pages-packages/spot-check",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "店面抽盘",
				"disableScroll": true
			}
		}, {
			"path": "add/index",
			"style": {
				"navigationBarTitleText": "店面抽盘",
				"disableScroll": true
			}
		}]
	}, {
		"root": "pages-packages/plan-question",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "计划巡检整改",
				"disableScroll": true
			}
		}]
	}, {
		"root": "pages-packages/plan-review",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "计划巡检复核",
				"disableScroll": true
			}
		}]
	}],
	"preloadRule": {
		"pages-packages/store/index/index": {
			"network": "all",
			"packages": ["pages-packages/store"]
		},
		"pages-packages/plan/index/index": {
			"network": "all",
			"packages": ["pages-packages/plan"]
		},
		"pages-packages/spot-check/index/index": {
			"network": "all",
			"packages": ["pages-packages/spot-check"]
		},
		"pages-packages/meeting/index/index": {
			"network": "all",
			"packages": ["pages-packages/meeting"]
		},
		"pages-packages/plan-question/index/index": {
			"network": "all",
			"packages": ["pages-packages/plan-question"]
		},
		"pages-packages/plan-review/index/index": {
			"network": "all",
			"packages": ["pages-packages/plan-review"]
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "汉光巡检系统",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#fff",
		"navigationStyle": "default"
	}
}
