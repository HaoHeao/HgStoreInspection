{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
			"path": "user/login/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		},
		{
			"path": "user/selectInfo/selectInfo",
			"style": {
				"navigationBarTitleText": "用户信息修改",
				"disableScroll": true
			}
		},
		{
			"path": "user/index/index",
			"style": {
				"disableScroll": true
			}
		}
	],
	"subPackages": [{
		"root": "pages",
		"pages": [{
				"path": "index/index",
				"style": {
					"navigationBarTitleText": "卖场巡检",
					"disableScroll": true
				}
			},
			{
				"path": "seeDetail/seeDetail",
				"style": {
					"navigationBarTitleText": "查看巡检记录",
					"disableScroll": true
				}
			},
			{
				"path": "viewQuestion/viewQuestion",
				"style": {
					"navigationBarTitleText": "问题讨论",
					"disableScroll": true
				}
			},
			{
				"path": "add/add",
				"style": {
					"navigationBarTitleText": "添加卖场巡检记录",
					"disableScroll": true
				}
			},
			{
				"path": "addSelect/addSelect",
				"style": {
					"disableScroll": true
				}
			}
		]
	}, {
		"root": "pages-plan",
		"pages": [{
				"path": "index/index",
				"style": {
					"navigationBarTitleText": "计划巡检",
					"disableScroll": true
				}
			}, {
				"path": "plan/plan",
				"style": {
					"navigationBarTitleText": "计划巡检内容",
					"disableScroll": true
				}
			}, {
				"path": "submitQuestions/submitQuestions",
				"style": {
					"navigationBarTitleText": "提出巡检问题",
					"disableScroll": true
				}
			},
			{
				"path": "question/question",
				"style": {
					"navigationBarTitleText": "巡检问题",
					"disableScroll": true
				}
			}
		]
	}, {
		"root": "pages-packages/meeting",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "会议预约",
				"disableScroll": true
			}
		}, {
			"path": "reserva/index",
			"style": {
				"navigationBarTitleText": "预约",
				"disableScroll": true
			}
		}, {
			"path": "detail/index",
			"style": {
				"navigationBarTitleText": "预约信息",
				"disableScroll": true
			}
		}]
	}, {
		"root": "pages-packages/spot-check",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "店面抽盘",
				"disableScroll": true
			}
		}, {
			"path": "add/index",
			"style": {
				"navigationBarTitleText": "店面抽盘",
				"disableScroll": true
			}
		}]
	}, {
		"root": "pages-packages/plan-question",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "计划巡检整改",
				"disableScroll": true
			}
		}]
	}, {
		"root": "pages-packages/plan-review",
		"pages": [{
			"path": "index/index",
			"style": {
				"navigationBarTitleText": "计划巡检复核",
				"disableScroll": true
			}
		}]
	}],
	"preloadRule": {
		"pages/index/index": {
			"network": "all",
			"packages": ["pages"]
		},
		"pages-plan/index/index": {
			"network": "all",
			"packages": ["pages-plan"]
		},
		"pages-packages/meeting/index/index": {
			"network": "all",
			"packages": ["pages-packages/meeting"]
		},
		"pages-packages/spot-check/index/index": {
			"network": "all",
			"packages": ["pages-packages/spot-check"]
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "汉光巡检系统",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#fff",
		"navigationStyle": "default"
	}
}
